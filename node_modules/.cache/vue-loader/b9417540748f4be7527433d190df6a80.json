a($color: $color-dark-gray, $alpha: 0.1);\n  border-radius: 1rem;\n  &-wrapper{\n  }\n  &-header{\n    padding: 1rem;\n    h3{\n      margin: 0;\n      font-size: 1.5rem;\n      color: #fff;\n    }\n  }\n  &-body{\n\n  }\n}\n</style>"]}]}