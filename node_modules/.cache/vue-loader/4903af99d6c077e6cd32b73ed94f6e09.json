 </div>\n</template>\n\n<script>\nimport AddTweet from '@/components/AddTweet'\nimport BaseIcon from '@/components/BaseIcon'\n\nexport default {\n  name: 'TweetPopup',\n  components: {\n    AddTweet,\n    BaseIcon\n  },\n  methods:{\n    handleClickOutside: function(e) {\n      const object = {\n        target: e.target, \n        ref: this.$refs.popupWrapper\n      }\n      if(object.target !== object.ref) return\n      this.$store.commit(\"toggleTweetButton\")\n    },\n    handleSubmit(){\n      this.$store.commit(\"toggleTweetButton\")\n      this.$store.commit(\"setMobileMenuState\", false)\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n@import '@/assets/theme/colors.scss';\n@import '@/assets/variables.scss';\n\n.tweet-popup{\n  position: fixed;\n  top: 0;\n  left: 0;\n  height: 100vh;\n  width: 100%;\n  z-index: 9999;\n  display: flex;\n  align-items: flex-start;\n  justify-content: center;\n  background-color: rgba($color: $color-dark-gray, $alpha: 0.3);\n  &-header{\n    padding: .5rem;\n    .close-button{\n      margin-left: auto;\n      width: 2rem;\n      width: 2rem;\n      padding: 5px;\n      cursor: pointer;\n      border-radius: 999px;\n      svg{\n        width: 100%;\n        height: 100%;\n        fill: $tweet-action-red;\n      }\n      &:hover{\n        background: rgba($color: $tweet-action-red, $alpha: 0.3);\n      }\n    }\n  }\n  &-wrapper{\n    margin-top: 50px;\n    border-radius: 1rem;\n    min-width: 450px;\n    background-color: $color-bg;\n  }\n}\n@media screen and (max-width: $phone) {\n  .tweet-popup{\n    &-wrapper{\n      min-width: unset;\n      width: 90%;\n    }\n  }\n}\n</style>"]}]}